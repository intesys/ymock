{
  "name": "@intesys/ymock",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "docs",
    "website",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev --filter=!ymock-docs --filter=!ymock-website",
    "clean": "turbo clean --no-cache --force --filter=ymock-client --filter=ymock-launcher --filter=ymock-host",
    "build": "turbo build --filter=ymock-client"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "prettier": "^2.8.0",
    "turbo": "^1.7.0"
  }
}
